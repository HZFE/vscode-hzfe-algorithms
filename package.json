{
  "name": "do-something-right",
  "displayName": "do-something-right",
  "description": "Manjusaka 独家公益刷题插件",
  "version": "0.0.7",
  "publisher": "do-something-right",
  "repository": {
    "url": "https://github.com/rottenpen/do-something-right"
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "do-something-right.sync",
        "title": "同步到公益项目",
        "category": "LeetCode"
      },
      {
        "command": "do-something-right.getDaily",
        "title": "获取每日一题 daily",
        "category": "LeetCode"
      }
    ],
    "configuration": [
      {
        "title": "do-something-right",
        "properties": {
          "do-something-right.wechatId": {
            "type": "string",
            "scope": "application",
            "description": "微信名"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
