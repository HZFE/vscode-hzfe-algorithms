{
  "name": "hzfe-algorithms",
  "displayName": "HZFE Algorithms",
  "description": "HZFE algorithms extension",
  "version": "0.0.1",
  "publisher": "hzfe",
  "repository": {
    "url": "https://github.com/HZFE/vscode-hzfe-algorithms"
  },
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hzfe-algorithms.sync",
        "title": "Sync to repo",
        "category": "HZFE Algorithm"
      },
      {
        "command": "hzfe-algorithms.config",
        "title": "Config plugin",
        "category": "HZFE Algorithm"
      }
    ],
    "configuration": {
      "title": "HZFE Algorithms",
      "properties": {
        "hzfe-algorithms.config.repo": {
          "type": "string",
          "scope": "application",
          "description": "Config github repo URL (Required)"
        },
        "hzfe-algorithms.config.nickname": {
          "type": "string",
          "scope": "application",
          "description": "Config nickname (Not Required)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "json-loader": "^0.5.7"
  }
}
